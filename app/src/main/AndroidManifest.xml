<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodzen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".CollectionActivities.SignUpSellerActivity"></activity>
        <activity
            android:name=".WalkthroughLayout.OnBoardActivity"
            android:exported="true" />
        <activity android:name=".CollectionActivities.DashboardActivity" />
        <activity android:name=".CollectionActivities.SignUpActivity" />
        <activity android:name=".CollectionActivities.SignInActivity" />
        <activity android:name=".CollectionActivities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>